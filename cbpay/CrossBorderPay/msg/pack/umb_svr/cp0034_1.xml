<?xml version="1.0" encoding="utf-8"?>
<formula id="cp0034_1">
<include>UMB_SVR_1</include>
	<field name="trancode" >
			<format type="s" length="6" default_value="cp0034" />
	</field>
	<include>UMB_SVR_2</include>
	
<field name="body">
	<format type="virtual" level="2" />
	</field>
	
	<field name="listsize">
		<format type="s" length="2" necessary="true" frommsg="true"  />
	</field>
	
<compositField name="exQuoteList" sizeRef="listsize" output="false">
			<field name="exQuoteList">
				<format type="virtual" level="1" />
			</field>
			<field name="account">
				<format type="s" length="32" necessary="true" />
			</field>
			<field name="accountname">
				<format type="s" length="32" necessary="true" />
			</field>

                        <field name="amount">
                               <format type="s" length="30" necessary="true" />
                         </field>
			<field name="rpflag">
				<format type="s" length="2" necessary="true" />
			</field>
			<field name="currency">
				<format type="s" length="6" necessary="true" />
			</field>
	                <field name="exrate">
				<format type="s" length="30" necessary="true" />
			</field>
			<field name="bak">
				<format type="s" length="256" necessary="true" />
			</field>
	</compositField>
</formula>
