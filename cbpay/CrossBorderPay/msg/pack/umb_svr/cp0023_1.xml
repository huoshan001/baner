<?xml version="1.0" encoding="utf-8"?>
<formula id="cp0023_1">
<include>UMB_SVR_1</include>
	<field name="trancode" >
	       <format type="s" length="6" default_value="CP0023" />
	</field>
	<include>UMB_SVR_2</include>
	
        <field name="body">
	       <format type="virtual" level="2" />
	</field>
        <field name="orderSize">
               <format type="s" length="10" necessary="true" />
        </field>

        <compositField name="orderList" sizeRef="orderSize" output="true">
         <field name="row">
	        <format type="virtual" level="1" />
	 </field>
         <field name="merchantNo">
                <format type="s" length="32" necessary="true" />
	 </field>
	 <field name="tranCode">
	        <format type="s" length="6" necessary="false" default_value="all" />
	 </field>
	 <field name="tranState">
		<format type="s" length="2" necessary="false" default_value="99"  />
	 </field>
         <field name="startDate">
                <format type="s" length="8" necessary="true" />
         </field>
         <field name="endDate">
                <format type="s" length="8" necessary="true" />
         </field>
	</compositField>
</formula>
